");\n            } else {\n                alert(\"Authentication failure\");\n            }\n        });\n    }\n\n    render() {\n        const {username, password} = this.state;\n\n\n        return (\n            <Spring\n                from={{ opacity: 0, marginTop: -1000 }}\n                to={{ opacity: 1, marginTop: 0 }}>\n                {props =>\n                    <div style={props}>\n                        <Card className={\"border border-dark bg-dark text-white cards\"}>\n                            <Card.Header align=\"center\"><h2>Login</h2></Card.Header>\n\n                            <Form onReset={this.resetUser} onSubmit={this.submitUser}>\n                                <Card.Body>\n                                    <Form.Row>\n                                        <Form.Group as={Col}>\n                                            <Form.Label>Username</Form.Label>\n                                            <Form.Control required autoComplete=\"off\"\n                                                          type=\"text\" name=\"username\"\n                                                          value={username}\n                                                          onChange={this.userChange}\n                                                          placeholder=\"Enter Username\"/>\n                                        </Form.Group>\n                                        <Form.Group as={Col}>\n                                            <Form.Label>Password</Form.Label>\n                                            <Form.Control required autoComplete=\"off\"\n                                                          type=\"password\" name=\"password\"\n                                                          value={password}\n                                                          onChange={this.userChange}\n                                                          placeholder=\"Enter Password\"/>\n                                        </Form.Group>\n                                    </Form.Row>\n                                </Card.Body>\n                                <Card.Footer align=\"right\">\n                                    <Button size=\"sm\" variant=\"success\" type=\"submit\">\n                                        Login\n                                    </Button>{' '}\n                                    <Button size=\"sm\" variant=\"info\" type=\"reset\">\n                                        Reset\n                                    </Button>\n                                </Card.Footer>\n                            </Form>\n                        </Card>\n                    </div>\n                }\n            </Spring>\n        )\n    }\n}\n","/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/components/Registration.js",[],"/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/components/Interceptors.js",[],"/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/components/MyToast.js",[],"/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/api/api.js",[],"/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/App.jsx",[],"/home/qwes/Documents/GitHub/ShopApp-Frontend-/reactjs/src/components/DashBoard.jsx",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":4,"column":9,"nodeType":"69","messageId":"70","endLine":4,"endColumn":17},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-unused-vars","'getUsers' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]